<h2 class = "text-center">Favorites</h2>



<% pokemon.forEach((p)=>{ %>
<div class="well">
  <h3>
    <a href="/pokemon/<%= p.name %>">
      <%= p.name %>
    </a>
  </h3>
  <form method="POST" action="/pokemon?_method=DELETE">
    <input type="hidden" name="name" value="<%= p.name %>" />
    <button id= "unfave" class="btn btn-primary yellow lighten-3" type="submit">Un-Fave?</button>
  </form>
</div>
<% }); %>